            "text/plain": [
              "      edad        ingresoanual      aniosempleo     historiacreditos\n",
              " Min.   :20.00   Min.   :  30000   Min.   : 0.000   Min.   : 2.000  \n",
              " 1st Qu.:23.00   1st Qu.:  42000   1st Qu.: 2.000   1st Qu.: 3.000  \n",
