        "calidad_resumen <- data.frame(      #Generamos una tabla que nos muestra el resumen de la sección 2.0 y la acción a realizar en la siguiente fase de preparación\n",
        "  Aspecto = c(\"Completitud\", \"Validez\", \"Consistencia\", \"Unicidad\", \"Precisión\"),\n",
        "  Estado = c(\"Verificado\", \"Outliers detectados\", \"Inconsistencias menores\",\n",
        "            \"No duplicados\", \"Rangos validados\"),\n",
        "  Accion_Requerida = c(\"OK\", \"Evaluar tratamiento\", \"Revisar casos específicos\",\n",
        "                      \"OK\", \"OK\")\n",
        ")\n",
        "\n",
        "print(\"=== TABLA RESUMEN CALIDAD ===\")\n",
        "print(calidad_resumen)"
      ]
    },
    {
      "cell_type": "markdown",
      "metadata": {
        "id": "MuC9Fr7np04C"
