        "#Árboles de decisión para monto_prestamo\n",
        "#establecer un punto de partida para el generador de números aleatorios.\n",
        "set.seed(123)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "KTqDQGjgxnTb"
      },
      "outputs": [],
      "source": [
        "# Árbol de decisión para monto_prestamo\n",
        "arbol_crecer <- rpart(monto_prestamo ~ edad + ingresoanual + aniosempleo + historiacreditos + tasainteres + tipovivienda + destinacion + tipoprestamo + estadodeuda + default + ponderador,\n",
        "                    data = crecer_train, method = \"anova\")"
      ]
    },
    {
