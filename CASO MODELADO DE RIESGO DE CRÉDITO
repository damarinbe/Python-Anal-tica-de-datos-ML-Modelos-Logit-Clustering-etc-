        "library(gridExtra) # Load the gridExtra package\n",
        "# Histogramas de variables objetivo\n",
        "hist_monto <- ggplot(crecer, aes(x = monto_prestamo)) +  #Visualizamos la variable de cupo\n",
        "  geom_histogram(bins = 50, fill = \"steelblue\", alpha = 0.7) +\n",
        "  labs(title = \"Distribución Monto del Préstamo\",   #En el entendimiento del negocio observamos que solo nos sirve el monto_prestamo<=15.000.000$cop\n",
        "       x = \"Monto (miles COP)\", y = \"Frecuencia\") + #En el gráfico sale una noción general del fondo y los montos destinados\n",
        "  theme_minimal()\n",
        "\n",
        "hist_default <- ggplot(crecer, aes(x = factor(default))) +\n",
        "  geom_bar(fill = \"coral\", alpha = 0.7) +\n",
        "  labs(title = \"Distribución Variable Default\",     #Con el boxplot de riesgo (default) podemos observar la necesidad de reducir la cartera pues se encuentra muy elevada\n",
        "       x = \"Default (0=No paga, 1=Sí paga)\", y = \"Frecuencia\") +\n",
        "  theme_minimal()\n",
        "\n",
        "grid.arrange(hist_monto, hist_default, ncol = 2,\n",
        "             top = \"Distribuciones Variables Objetivo\")"
      ]
